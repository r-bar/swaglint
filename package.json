{
	"name": "swaglint",
	"version": "0.1.5",
	"description": "Yet another swagger linter",
	"main": "./lib/index.js",
	"typings": "./lib/index.d.ts",
	"directories": {
		"bin": "./bin/",
		"doc": "./doc/",
		"test": "./test/"
	},
	"files": [
		"bin",
		"lib",
		"LICENSE.md"
	],
	"scripts": {
		"build": "rm -rf ./lib/ && $(npm bin)/tsc && $(npm bin)/babel ./lib/ --out-dir ./lib/",
		"test": "npm run test:style",
		"test:style": "$(npm bin)/tslint --format stylish ./src/**/*.ts",
		"prepublish": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bycedric/swaglint.git"
	},
	"keywords": [
		"open",
		"api",
		"swagger",
		"lint"
	],
	"author": "Cedric van Putten <me@bycedric.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bycedric/swaglint/issues"
	},
	"homepage": "https://github.com/bycedric/swaglint#readme",
	"dependencies": {
		"chalk": "^1.1",
		"commander": "^2.9",
		"concat-stream": "^1.6",
		"sway": "apigee-127/sway#cbc1dd184a50f833f2c85cb5cc748213dcb0c55d",
		"text-table": "^0.2",
		"yaml-js": "^0.1"
	},
	"devDependencies": {
		"@types/chalk": "^0.4",
		"@types/commander": "^2.3",
		"@types/node": "7.0",
		"babel-cli": "^6.24",
		"babel-preset-env": "^1.3",
		"tslint": "^5.1",
		"typescript": "^2.3"
	}
}
